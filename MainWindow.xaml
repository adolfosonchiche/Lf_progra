<Window x:Class="Lf_progra.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lf_progra"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="528" Background="{x:Null}">
    <Grid Margin="0,0,2,0">
        <Label x:Name="labelTextoIngreseCadena" Content="INGRESE UNA CADENA DE CARACTERES:&#xD;&#xA;" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Height="22.92"/>
        <TextBox x:Name="txtExpresion" HorizontalAlignment="Left" Height="18" Margin="20,37.92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="329.913"/>
        <Button x:Name="btnAnalizar" Content="ANALIZAR" HorizontalAlignment="Left" Height="29" Margin="372.913,26.92,0,0" VerticalAlignment="Top" Width="85" Click="Boton_Analizar"/>
        <Button x:Name="BtnLimpiar" Content="LIMPIAR DATOS" HorizontalAlignment="Left" Height="30" Margin="20,337,0,0" VerticalAlignment="Top" Width="142" Click="Boton_Limpiar"/>
        <TextBox x:Name="txtResultados" HorizontalAlignment="Left" Height="227" Margin="20,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="437.913"/>

    </Grid>
</Window>
